<title>Echo server</title>
<script src="socket.io/socket.io.js"></script>

<div class="pad-top notes">
  <ul class="pages">
    <li class="chat page">
      <div class="chatArea">
        <ul class="messages"></ul>
      </div>
      <input class="inputMessage" placeholder="Type here..."/>
    </li>
    <li class="login page">
      <div class="form">
        <h3 class="title">What username do you want to use, {{ user.first_name }}?</h3>
        <input class="usernameInput" type="text" maxlength="14" value="{{user.first_name}}"/>
      </div>
    </li>
  </ul>
</div>
